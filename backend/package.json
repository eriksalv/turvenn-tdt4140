{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express API",
  "main": "server.js",
  "author": "Andreas Netteland",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -L  server.js",
    "db:migrate": "docker exec backend sequelize db:migrate",
    "db:migrate:undo": "docker exec backend sequelize db:migrate:undo",
    "db:migrate:undo:all": "docker exec backend sequelize db:migrate:undo:all",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\""
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.1"
  },
  "devDependencies": {
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1"
